{{#partial "header"}}
	<tr>
		<td align="center" class="em_white" style="font-family:'Karla', 'Open Sans', 'Roboto', Arial, sans-serif; font-size:36px; line-height:40px; color:#ffffff; font-weight:bold;">Appointment booked</td>
	</tr>
{{/partial}}

{{#partial "body"}}
	<tr>
		<td align="left" class="em_blk" style="font-family:'Karla', 'Open Sans', 'Roboto', Arial, sans-serif; font-size:20px; line-height:24px; color:#000000; font-weight:bold;">Hi {{providerName}},</td>
	</tr>
	<tr>
		<td height="20" style="height:20px; line-height:0px; font-size:0px;" class="em_h20">&nbsp;</td>
	</tr>
	<tr>
		<td align="left" class="em_blue" style="font-family:'Karla', 'Open Sans', 'Roboto', Arial, sans-serif; font-size:18px; line-height:24px; color:#000;">
			<p>
				<b>{{accountName}} ({{accountEmailAddress}})</b> will be meeting with you at <a href="{{{videoconferenceUrl}}}">{{{videoconferenceUrl}}}</a> on <b>{{appointmentStartDateDescription}}</b> @ <b>{{appointmentStartTimeDescription}}</b>
			</p>
		</td>
	</tr>
{{/partial}}

{{> layouts/en/default}}